<View xmlns="sap.m" 
    xmlns:core="sap.ui.core" controllerName="sap.ushell.components.shell.SubMenu.controller.MenuBar">
    <IconTabHeader showOverflowSelectList="true" items="{menu>/}" select=".onMenuItemSelection">
        <items>
            <IconTabFilter text="{menu>text}" key="{menu>intent}" />
        </items>
    </IconTabHeader>
</View>
