{
	"_version": "1.14.0",
	"sap.app": {
		"id": "sap.fe.templates.ObjectPage",
		"type": "component",
		"i18n": "../../messagebundle.properties",
		"applicationVersion": {
			"version": "1.71.0"
		},
		"title": "{{TITLE_OP}}",
		"tags": {
			"keywords": [
				"{{TITLE_OP}}"
			]
		},
		"ach": "CA-UI5-ST",
		"offline": false,
		"resources": "resources.json"
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal",
			"sap_belize",
			"sap_belize_plus",
		  	"sap_belize_hcw"
		]
	},
	"sap.ui5": {
		"config": {
			"sapFiori2Adaptation": {
				"style": true,
				"hierarchy": true
			}
		},
		"services": {
			"ShellUIService": {
				"factoryName": "sap.ushell.ui5service.ShellUIService"
			},
			"namedBindingModel": {
				"factoryName": "sap.fe.services.NamedBindingModelService",
				"startup": "waitFor"
			},
			"templatedView": {
				"factoryName": "sap.fe.services.TemplatedViewService",
				"startup": "waitFor",
				"settings": {
					"viewName": "sap.fe.templates.ObjectPage.ObjectPage"
				}
			}
		},
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "1.56.0",
			"libs": {
				"sap.f": {
					"minVersion": "1.56.0"
				},
				"sap.fe.macros": {
					"minVersion": "1.68.0",
					"lazy": true
				},
				"sap.m": {
					"minVersion": "1.56.0"
				},
				"sap.suite.ui.microchart": {
					"minVersion": "1.56.0"
				},
				"sap.ui.core": {
					"minVersion": "1.56.0"
				},
				"sap.ui.layout": {
					"minVersion": "1.56.0"
				},
				"sap.ui.mdc": {
					"minVersion": "1.56.0"
				},
				"sap.uxap": {
					"minVersion": "1.56.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	}
}